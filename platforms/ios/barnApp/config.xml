<?xml version='1.0' encoding='utf-8'?>
<widget id="se.produktionproduktion.barnApp" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="SplashScreen">
        <param name="ios-package" value="CDVSplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Console">
        <param name="ios-package" value="CDVLogger" />
    </feature>
    <feature name="File">
        <param name="ios-package" value="CDVFile" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Media">
        <param name="ios-package" value="CDVSound" />
    </feature>
    <icon height="180" src="resources/icon/ios/Icon-60@3x.png" width="180" />
    <icon height="60" src="resources/icon/ios/Icon-60.png" width="60" />
    <icon height="120" src="resources/icon/ios/Icon-60@2x.png" width="120" />
    <icon height="76" src="resources/icon/ios/Icon-76.png" width="76" />
    <icon height="152" src="resources/icon/ios/Icon-76@2x.png" width="152" />
    <icon height="40" src="resources/icon/ios/Icon-40.png" width="40" />
    <icon height="80" src="resources/icon/ios/Icon-40@2x.png" width="80" />
    <icon height="57" src="resources/icon/Icon.png" width="57" />
    <icon height="114" src="resources/icon/Icon@2x.png" width="114" />
    <icon height="72" src="resources/icon/ios/Icon-72.png" width="72" />
    <icon height="144" src="resources/icon/ios/Icon-72@2x.png" width="144" />
    <icon height="29" src="resources/icon/ios/Icon-Small.png" width="29" />
    <icon height="58" src="resources/icon/ios/Icon-Small@2x.png" width="58" />
    <icon height="50" src="resources/icon/ios/Icon-50.png" width="50" />
    <icon height="100" src="resources/icon/ios/Icon-50@2x.png" width="100" />
    <splash height="1136" src="resources/splash/ios/Default-568h@2x.png" width="640" />
    <splash height="1334" src="resources/splash/ios/Default-667h@2x.png" width="750" />
    <splash height="2208" src="resources/splash/ios/Default-Portrait-736h@3x.png" width="1242" />
    <splash height="1024" src="resources/splash/ios/Default-Portrait.png" width="768" />
    <splash height="2048" src="resources/splash/ios/Default-Portrait@2x.png" width="1536" />
    <splash height="480" src="resources/splash/ios/Default.png" width="320" />
    <splash height="960" src="resources/splash/ios/Default@2x.png" width="640" />
    <name>barnApp</name>
    <description>Pekbok med rim</description>
    <author email="info@produktionproduktion.se" href="http://produktionproduktion.se">ProduktionProduktion</author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="FadeSplashScreen" value="true" />
    <preference name="FadeSplashScreenDuration" value="0" />
    <preference name="ShowSplashScreenSpinner" value="true" />
    <preference name="AllowInlineMediaPlayback" value="true" />
</widget>
